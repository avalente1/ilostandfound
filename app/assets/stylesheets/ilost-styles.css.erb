  border-color: #facb26;
}


.navbar-ilostnav {
  background-color: #fff;
  border-color: 0;
}

.navbar-ilostnav .navbar-brand {
  color: #003441;
}

.navbar-ilostnav .navbar-brand:hover,
.navbar-ilostnav .navbar-brand:focus {
  color: #ffffff;
  background-color: #4ab9d4;
}

.navbar-ilostnav .navbar-text {
  color: #003441;
}

.navbar-ilostnav .navbar-nav > li > a {
  color: #003441;
}

.navbar-ilostnav .navbar-nav > li > a:hover,
.navbar-ilostnav .navbar-nav > li > a:focus {
  color: #fff;
  background-color: #4ab9d4;
}

.navbar-ilostnav .navbar-nav > .active > a,
.navbar-ilostnav .navbar-nav > .active > a:hover,
.navbar-ilostnav .navbar-nav > .active > a:focus {
  color: #fff;
  background-color: #4ab9d4;
}

.navbar-ilostnav .navbar-nav > .disabled > a,
.navbar-ilostnav .navbar-nav > .disabled > a:hover,
.navbar-ilostnav .navbar-nav > .disabled > a:focus {
  color: #444444;
  background-color: transparent;
}

.navbar-ilostnav .navbar-toggle {
  border-color: #333333;
}

.navbar-ilostnav .navbar-toggle:hover,
.navbar-ilostnav .navbar-toggle:focus {
  background-color: #ccc;
}

.navbar-ilostnav .navbar-toggle .icon-bar {
  background-color: #4ab9d4;
}

.navbar-ilostnav .navbar-collapse,
.navbar-ilostnav .navbar-form {
  border-color: #666;
}

.navbar-ilostnav .navbar-nav > .open > a,
.navbar-ilostnav .navbar-nav > .open > a:hover,
.navbar-ilostnav .navbar-nav > .open > a:focus {
  color: #ffffff;
  background-color: #4ab9d4;
}

.navbar-ilostnav .navbar-nav > .dropdown > a:hover .caret {
  border-top-color: #ccc;
  border-bottom-color: #666;
}

.navbar-ilostnav .navbar-nav > .dropdown > a .caret {
  border-top-color: #003441;
  border-bottom-color: #003441;
}

.navbar-ilostnav .navbar-nav > .open > a .caret,
.navbar-ilostnav .navbar-nav > .open > a:hover .caret,
.navbar-ilostnav .navbar-nav > .open > a:focus .caret {
  border-top-color: #ffffff;
  border-bottom-color: #ffffff;
}

@media (max-width: 767px) {
  .navbar-ilostnav .navbar-nav .open .dropdown-menu > .dropdown-header {
    border-color: #080808;
  }
  .navbar-ilostnav .navbar-nav .open .dropdown-menu > li > a {
    color: #003441;
  }
  .navbar-ilostnav .navbar-nav .open .dropdown-menu > li > a:hover,
  .navbar-ilostnav .navbar-nav .open .dropdown-menu > li > a:focus {
    color: #333;
    background-color: #FFF;
  }
  .navbar-ilostnav .navbar-nav .open .dropdown-menu > .active > a,
  .navbar-ilostnav .navbar-nav .open .dropdown-menu > .active > a:hover,
  .navbar-ilostnav .navbar-nav .open .dropdown-menu > .active > a:focus {
    color: #333;
    background-color: #ccc;
  }
  .navbar-ilostnav .navbar-nav .open .dropdown-menu > .disabled > a,
  .navbar-ilostnav .navbar-nav .open .dropdown-menu > .disabled > a:hover,
  .navbar-ilostnav .navbar-nav .open .dropdown-menu > .disabled > a:focus {
    color: #444444;
    background-color: #FFF;
  }

}

.navbar-ilostnav .navbar-link {
  color: #003441;
}

.navbar-ilostnav .navbar-link:hover {
  color: #999;
}



